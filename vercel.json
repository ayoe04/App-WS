{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-pdf",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}